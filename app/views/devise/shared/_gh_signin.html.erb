<%- if devise_mapping.omniauthable? %>
  <%- resource_class.omniauth_providers.each do |provider| %>
    <%= link_to omniauth_authorize_path(resource_name, provider), class: "btn btn-block btn-dark" do %>
    <span class="mr-2">Developer Sign in</span><i class="fab fa-github"></i>
    <% end %>
  <% end %>
<% end %>
